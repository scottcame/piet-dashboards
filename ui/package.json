{
  "name": "piet-dashboards",
  "version": "1.0.0",
  "description": "Dashboard Functionality for Piet",
  "scripts": {
    "clean": "rm public/bundle.*",
    "build": "rollup -c --environment DEV:false,REMOTE:true",
    "build-dev": "rollup -c --environment DEV:true,REMOTE:true",
    "build-local-dev": "rollup -c --environment DEV:true,REMOTE:false",
    "autobuild": "rollup -c -w --environment DEV:true,REMOTE:false",
    "autobuild-playground": "rollup -c -w --environment DEV:true,PLAYGROUND:true",
    "dev": "run-p start:dev autobuild",
    "playground": "run-p start:dev autobuild-playground",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "lint": "eslint --ext ts src/js test",
    "test": "run-s lint jest",
    "build:tailwind": "tailwind build tailwind.css -o public/tailwind-gen.css"
  },
  "author": "Scott Came",
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@tsconfig/svelte": "^1.0.10",
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.10",
    "eslint-plugin-jest": "^23.20.0",
    "fs": "0.0.1-security",
    "jest": "^26.4.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "rollup": "^2.26.9",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.1",
    "rollup-plugin-typescript": "^1.0.1",
    "sass": "^1.26.10",
    "sirv-cli": "^1.0.6",
    "svelte": "^3.24.1",
    "svelte-check": "^1.0.33",
    "svelte-preprocess": "^4.2.1",
    "tailwindcss": "^1.7.6",
    "ts-jest": "^26.3.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2",
    "vega": "^5.15.0",
    "vega-embed": "^6.11.1",
    "vega-lite": "^4.15.0"
  }
}
