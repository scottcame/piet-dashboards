{
  "title": {
    "short": "Piet Dashboard",
    "long": "Demo Piet Dashboard"
  },
  "appLogoImageUrl": "img/piet-logo.jpg",
  "allowVizExport": false,
  "groups": [
    {
      "header": "Visualization Group 1",
      "visualizations": {
        "viz-1-1": {
          "vizType": "bar",
          "panelTitle": "Bar: Units Ordered by state",
          "headerText": "Units Ordered by state (bar chart)",
          "connection": "foodmart",
          "query": "SELECT NON EMPTY {[Measures].[Units Ordered]} * Union({[Store].[Stores].[Store State].Hierarchy.FirstChild.Parent}, Except({[Store].[Stores].[Store State].Members}, {})) ON COLUMNS FROM [Warehouse]",
          "measures": ["Units Ordered"],
          "xDimension": "[Store].[Stores].[Store State]"
        },
        "viz-1-2": {
          "vizType": "bar",
          "panelTitle": "Bar: Units Ordered by state excluding Oregon",
          "headerText": "Units Ordered by state (bar chart), excluding Oregon",
          "connection": "foodmart",
          "query": "SELECT NON EMPTY {[Measures].[Units Ordered]} * Union({[Store].[Stores].[Store State].Hierarchy.FirstChild.Parent}, Except({[Store].[Stores].[Store State].Members}, {[Store].[Stores].[Store State].[OR]})) ON COLUMNS FROM [Warehouse]",
          "measures": ["Units Ordered"],
          "xDimension": "[Store].[Stores].[Store State]"
        },
        "viz-1-3": {
          "vizType": "pie",
          "panelTitle": "Pie: Units Ordered by state excluding Oregon",
          "headerText": "Units Ordered by state (pie chart), excluding Oregon",
          "connection": "foodmart",
          "query": "SELECT NON EMPTY {[Measures].[Units Ordered]} * Union({[Store].[Stores].[Store State].Hierarchy.FirstChild.Parent}, Except({[Store].[Stores].[Store State].Members}, {})) ON COLUMNS FROM [Warehouse]",
          "measures": ["Units Ordered"],
          "measureFormats": [","],
          "xDimension": "[Store].[Stores].[Store State]"
        }
      }
    },
    {
      "header": "Visualization Group 2",
      "visualizations": {
        "viz-2-1": {
          "vizType": "line",
          "panelTitle": "Line 1",
          "headerText": "Line chart 1 (simple ordinal)",
          "connection": "foodmart",
          "query": "TEST_QUERY_LINE_SIMPLE",
          "measures": ["y"],
          "xDimension": "x"
        },
        "viz-2-2": {
          "vizType": "line",
          "panelTitle": "Line 2",
          "headerText": "Line chart 2 (two measures)",
          "connection": "foodmart",
          "query": "TEST_QUERY_LINE_2_MEASURES",
          "measures": ["y", "z"],
          "xDimension": "x"
        },
        "viz-2-3": {
          "vizType": "line",
          "panelTitle": "Line Temporal Year",
          "headerText": "Line Temporal Year",
          "connection": "foodmart",
          "query": "TEST_QUERY_LINE_TEMPORAL_1_MEASURE_YEAR",
          "measures": ["y"],
          "xDimension": {
            "yearDimension": "x"
          }
        },
        "viz-2-4": {
          "vizType": "line",
          "panelTitle": "Line Temporal Year+Month",
          "headerText": "Line Temporal Year+Month",
          "connection": "foodmart",
          "query": "TEST_QUERY_LINE_TEMPORAL_1_MEASURE_YEAR_MONTH",
          "measures": ["y"],
          "xDimension": {
            "yearDimension": "yr",
            "monthDimension": "month"
          },
          "showPoints": true
        },
        "viz-2-5": {
          "vizType": "line",
          "panelTitle": "Line Temporal Year+Month+Day",
          "headerText": "Line Temporal Year+Month+Day",
          "connection": "foodmart",
          "query": "TEST_QUERY_LINE_TEMPORAL_1_MEASURE_YEAR_MONTH_DAY",
          "measures": ["y"],
          "xDimension": {
            "yearDimension": "yr",
            "monthDimension": "month",
            "dayDimension": "da day"
          },
          "showPoints": true
        },
        "viz-2-6": {
          "vizType": "line",
          "panelTitle": "Line Temporal Date",
          "headerText": "Line Temporal Date",
          "connection": "foodmart",
          "query": "TEST_QUERY_LINE_TEMPORAL_1_MEASURE_DATE",
          "measures": ["y"],
          "measureLabels": ["Some Y"],
          "xDimension": {
            "dateDimension": "date"
          },
          "showPoints": true,
          "dateFormat": "%m-%d"
        }
      }
    }
  ]
}