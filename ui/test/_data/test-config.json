{
  "title": {
    "short": "Piet Dashboard",
    "long": "Demo Piet Dashboard"
  },
  "appLogoImageUrl": "img/piet-logo.jpg",
  "allowVizExport": false,
  "groups": [
    {
      "header": "Visualization Group 1",
      "visualizations": {
        "viz-1-1": {
          "panelTitle": "Bar: Units ordered by state",
          "showEmpty": false,
          "connection": "foodmart",
          "cube": "[Warehouse]",
          "measure": "Units Ordered",
          "xDimension": "[Store].[Stores].[Store State]",
          "xDimensionExcludes": [],
          "vizType": "bar",
          "text": "Units ordered by state (bar chart)"
        },
        "viz-1-2": {
          "panelTitle": "Bar: Units ordered by state exclusing OR",
          "showEmpty": false,
          "connection": "foodmart",
          "cube": "[Warehouse]",
          "measure": "Units Ordered",
          "xDimension": "[Store].[Stores].[Store State]",
          "xDimensionExcludes": ["[Store].[Stores].[Store State].[OR]"],
          "vizType": "bar",
          "text": "Units ordered by state, excluding Oregon (bar chart)"
        },
        "viz-1-3": {
          "panelTitle": "Pie: Units ordered by state",
          "showEmpty": false,
          "connection": "foodmart",
          "cube": "[Warehouse]",
          "measure": "Units Ordered",
          "xDimension": "[Store].[Stores].[Store State]",
          "xDimensionExcludes": [],
          "vizType": "pie",
          "text": "Units ordered by state (pie chart)"
        }
      }
    },
    {
      "header": "Visualization Group 2",
      "visualizations": {
        "viz-2-1": {
          "panelTitle": "Heatgrid: Sq ft by store country x store type",
          "showEmpty": true,
          "connection": "foodmart",
          "cube": "[Store]",
          "measure": "Store Sqft",
          "measureLabel": "Store size in sq ft",
          "xDimension": "[Store Type].[Store Type].[Store Type]",
          "xDimensionExcludes": [],
          "yDimension": "[Store].[Stores].[Store Country]",
          "yDimensionExcludes": [],
          "vizType": "heatgrid",
          "text": "These are the values of M1 for D1xD2"
        },
        "viz-2-2": {
          "panelTitle": "Timeline: monthly sales",
          "showEmpty": true,
          "connection": "foodmart",
          "cube": "[Sales]",
          "measure": "Unit Sales",
          "xDimension": "[Store].[Stores].[Store Country]",
          "xDimensionExcludes": [],
          "vizType": "timeline",
          "text": "These are the values of M1 for D1xD2"
        }

      }
    }
  ]
}